{
  "containerDefinitions": [
    {
      "name": "fe",
      "image": "public.ecr.aws/nginx/nginx:stable-perl",
      "cpu": 0,
      "portMappings": [
        {
          "name": "techunity-fe-3000-tcp",
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "DATABASE_URL",
          "value": "${DATABASE_URL}"
        },
        {
          "name": "NEXT_PUBLIC_SUPABASE_ANON_KEY",
          "value": "${NEXT_PUBLIC_SUPABASE_ANON_KEY}"
        },
        {
          "name": "NEXT_PUBLIC_SUPABASE_URL",
          "value": "${NEXT_PUBLIC_SUPABASE_URL}"
        },
        {
          "name": "AUTH_GITHUB_CLIENT_ID",
          "value": "${AUTH_GITHUB_CLIENT_ID}"
        },
        {
          "name": "AUTH_GITHUB_SECRET",
          "value": "${AUTH_GITHUB_SECRET}"
        },
        {
          "name": "GOOGLE_CLIENT_ID",
          "value": "${GOOGLE_CLIENT_ID}"
        },
        {
          "name": "GOOGLE_SECRET",
          "value": "${GOOGLE_SECRET}"
        },
        {
          "name": "NEXTAUTH_SECRET",
          "value": "${NEXTAUTH_SECRET}"
        },
        {
          "name": "NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME",
          "value": "${NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME}"
        },
        {
          "name": "NEXT_PUBLIC_CLOUDINARY_UPLOAD_PRESET",
          "value": "${NEXT_PUBLIC_CLOUDINARY_UPLOAD_PRESET}"
        },
        {
          "name": "CLOUDINARY_URL",
          "value": "${CLOUDINARY_URL}"
        },
        {
          "name": "NEXT_PUBLIC_CLOUDINARY_API_KEY",
          "value": "${NEXT_PUBLIC_CLOUDINARY_API_KEY}"
        },
        {
          "name": "CLOUDINARY_API_SECRET",
          "value": "${CLOUDINARY_API_SECRET}"
        },
        {
          "name": "NEXTAUTH_URL",
          "value": "${NEXTAUTH_URL}"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/techunity-fe",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "family": "techunity-fe",
  "executionRoleArn": "ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
